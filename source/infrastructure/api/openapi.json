{
    "openapi": "3.1.0",
    "info": {
        "title": "ETL Processing Service",
        "version": "0.1.0"
    },
    "paths": {
        "/process": {
            "post": {
                "summary": "Extract information from a file",
                "description": "Main ETL processing endpoint",
                "operationId": "extract_file",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "required": [
                                    "s3_bucket",
                                    "s3_prefix"
                                ],
                                "properties": {
                                    "s3_bucket": {
                                        "type": "string",
                                        "title": "S3 Bucket"
                                    },
                                    "s3_prefix": {
                                        "type": "string",
                                        "title": "S3 Prefix"
                                    }
                                },
                                "title": "ETLRequest",
                                "description": "Request model for ETL processing"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                }
            }
        }
    }
}